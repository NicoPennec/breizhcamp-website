{{ define "response" }}
{{- $.Scratch.Add "index" slice -}}
{{- range sort (where $.Site.Data.sponsors.items "breizhcamp_level" "in" (slice "platinum" "gold" "silver" "bronze"))  "name" -}}
{{- $.Scratch.Add "index" (dict "name" .name "logo" (printf "%s%s" (strings.TrimSuffix "/" $.Site.BaseURL) .logo) "url" .url "level" .breizhcamp_level) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
{{ end }}